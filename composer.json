{
    "name": "laravelista/lissandra",
    "description": "API for Laracasts Feed built with Lumen.",
    "keywords": ["api", "laravel", "laracasts", "feed", "json", "xml"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "sabre/xml": "0.4.*",
        "league/fractal": "0.12.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
